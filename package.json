  "scripts": {
    "betterauth:generate": "@better-auth/cli generate --config src/lib/auth/server.ts",
    "biome:check": "biome check .",
    "biome:check:fix": "biome check --write .",
    "biome:format:fix": "biome format --write .",
    "build": "drizzle-kit generate --config src/drizzle.config.ts && drizzle-kit push --config src/drizzle.config.ts && next build",
    "check": "next lint && tsc --noEmit",
    "db:build": "drizzle-kit generate --config src/drizzle.config.ts && drizzle-kit push --config src/drizzle.config.ts && next build",
    "db:generate": "drizzle-kit generate --config src/drizzle.config.ts",
    "db:migrate": "drizzle-kit migrate --config src/drizzle.config.ts",
    "db:push": "drizzle-kit push --config src/drizzle.config.ts",
    "db:studio": "drizzle-kit studio --config src/drizzle.config.ts",
    "dev": "next dev",
    "drizzle:build": "drizzle-kit generate --config src/drizzle.config.ts && drizzle-kit push --config src/drizzle.config.ts && next build",
    "drizzle:generate": "drizzle-kit generate --config src/drizzle.config.ts",
    "drizzle:migrate": "drizzle-kit migrate --config src/drizzle.config.ts",
    "drizzle:push": "drizzle-kit push --config src/drizzle.config.ts",
    "drizzle:studio": "drizzle-kit studio --config src/drizzle.config.ts",
    "lint": "tsc --noEmit -p tsconfig.json && biome lint --diagnostic-level=error --no-errors-on-unmatched ./**/*.{js,ts}",
    "lint:fix": "biome check --apply-unsafe ./**/*.{js,ts}",
    "postbuild": "next-sitemap --config next-sitemap.config.cjs",
    "preview": "next build && next start",
    "prisma:build": "prisma generate && prisma db push && next build",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:migrate:deploy": "prisma migrate deploy",
    "prisma:push": "prisma db push --skip-generate",
    "prisma:studio": "prisma studio",
    "start": "next start",
    "typecheck": "tsc --noEmit --incremental false"
  },
